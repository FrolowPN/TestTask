<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="UsersFile" value="D:\VS\TestTaskFine\WorklistAssistant\BL\Resources\users.txt" />
  </appSettings>

  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IWAService1" />
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IWAService">
          <reliableSession enabled="true" />
          <security mode="Message">
            <message clientCredentialType="UserName" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:63412/Service.svc/WAService"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWAService1"
        contract="WAServiceOnIIS.IWAService" name="BasicHttpBinding_IWAService1" />
      <endpoint address="net.tcp://localhost:8080/WAService" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_IWAService" contract="WAService.IWAService"
        name="NetTcpBinding_IWAService">
        <identity>
          <dns value="MyCert" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>